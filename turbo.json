{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "web#build": {
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_GQL_SERVER_URI"
      ],
      "outputs": [".next/**"]
    },
    "admin#build": {
      "dependsOn": [
        "^build",
        "$NEXT_PUBLIC_GQL_SERVER_URI"
      ],
      "outputs": [".next/**"]
    },
    "api#build": {
      "dependsOn": [
        "^build",
        "$DATABASE_URL"
      ]
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
